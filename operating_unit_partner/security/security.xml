<odoo>

    <record id="ir_rule_product_template_allowed_operating_units" model="ir.rule">
        <field name="model_id" ref="base.model_res_partner" />
        <field name="domain_force">[]</field>
        <field name="name">Partners allowed operating units</field>
        <field name="global" eval="True" />
        <field eval="0" name="perm_unlink" />
        <field eval="0" name="perm_write" />
        <field eval="1" name="perm_read" />
        <field eval="0" name="perm_create" />
    </record>

    <record id="ir_rule_product_category_allowed_operating_units" model="ir.rule">
        <field name="model_id" ref="base.model_res_country" />
        <field name="domain_force">[]</field>
        <field name="name">Countries allowed operating units</field>
        <field name="global" eval="True" />
        <field eval="0" name="perm_unlink" />
        <field eval="0" name="perm_write" />
        <field eval="1" name="perm_read" />
        <field eval="0" name="perm_create" />
    </record>

</odoo>
