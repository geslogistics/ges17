<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!--Sequence -->
        <record id="partner_application_sequence" model="ir.sequence">
            <field name="name">Partner Application</field>
            <field name="code">res.partner.application</field>
            <field name="prefix">/%(year)s/%(month)s/</field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="res_partner_sequence" model="ir.sequence">
            <field name="name">Partner Sequence</field>
            <field name="code">res.partner.uniqseq</field>
            <field name="prefix"></field>
            <field name="padding"></field>
            <field name="number_next">3000</field>
            <field name="number_increment">16</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>