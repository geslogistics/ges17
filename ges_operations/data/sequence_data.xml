<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <data noupdate="1">

        <!--Sequence -->
        <record id="shipment_order_sequence" model="ir.sequence">
            <field name="name">Shipment Order</field>
            <field name="code">shipment.order</field>
            <field name="prefix">/%(year)s/%(month)s/</field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="transport_order_sequence" model="ir.sequence">
            <field name="name">Transport Order</field>
            <field name="code">transport.order</field>
