                                <field name="destination_delivery_address_city_id"
                                       invisible="transport not in 'road' and not delivery_option" readonly="not destination_delivery_address_id"
                                       required="destination_delivery_address_save_option"
                                       force_save="1" options="{'no_open': True, 'no_quick_create': True, 'no_create': True}"/>
                                <field name="destination_delivery_address_zip_code"
                                       invisible="transport not in 'road' and not delivery_option" readonly="not destination_delivery_address_id"
                                       force_save="1" options="{'no_open': True, 'no_quick_create': True, 'no_create': True}"/>
                                <field name="destination_delivery_address_street"
                                       invisible="transport not in 'road' and not delivery_option" readonly="not destination_delivery_address_id"
                                       required="destination_delivery_address_save_option"
                                       force_save="1" options="{'no_open': True, 'no_quick_create': True, 'no_create': True}"/>
                                <field name="destination_delivery_address_street2"
                                       invisible="transport not in 'road' and not delivery_option" readonly="not destination_delivery_address_id"
                                       force_save="1" options="{'no_open': True, 'no_quick_create': True, 'no_create': True}"/>
                                <field name="destination_delivery_address_street3"
                                       invisible="transport not in 'road' and not delivery_option" readonly="not destination_delivery_address_id"
                                       force_save="1" options="{'no_open': True, 'no_quick_create': True, 'no_create': True}"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="General Information">
                                <group string="Tracking Details">
                                    <field name="tracking_number"/>
                                </group>
                            </page>
                            <page string="Agent">
                                <group>
                                    <field name="agent_id"/>
                                    <field name="agent_email"/>
                                    <field name="agent_phone"/>
                                    <field name="agent_mobile"/>
                                </group>
                            </page>
                            <page string="Services">
                                <field name="sol_ids"
                                       options="{'no_open': True, 'no_quick_create': True, 'no_create': True}">
                                    <tree create="0" edit="0" delete="0">
                                        <field name="product_template_id"/>
                                        <field name="name"/>
                                        <field name="product_uom_qty"/>
                                        <field name="product_uom"/>
                                        <field name="price_unit"/>
