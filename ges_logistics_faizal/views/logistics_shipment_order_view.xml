                                <field name="origin_pickup_address_street2"
                                       invisible="transport not in 'road' and not pickup_option" readonly="origin_pickup_address_id"
                                       force_save="1"
                                       options="{'no_open': True, 'no_quick_create': True, 'no_create': True}"/>
                                <field name="origin_pickup_address_street3"
