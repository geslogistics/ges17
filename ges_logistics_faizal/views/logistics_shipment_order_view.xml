                                <field name="destination_delivery_address_save_option"
                                       invisible="not delivery_option or destination_delivery_address_id"/>
                                <field name="destination_delivery_address_name"
                                       invisible="transport not in 'road' and not delivery_option" readonly="not destination_delivery_address_id"
