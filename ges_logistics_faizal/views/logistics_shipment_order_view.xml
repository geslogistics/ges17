                                       force_save="1" options="{'no_open': True, 'no_quick_create': True, 'no_create': True}"/>
                                <field name="destination_delivery_address_country_id"
                                       invisible="transport not in 'road' and not delivery_option" required="destination_delivery_address_save_option"
                                       readonly="1" force_save="1" options="{'no_open': True, 'no_quick_create': True, 'no_create': True}"/>
                                <field name="destination_delivery_address_state_id"
                                       invisible="transport not in 'road' and not delivery_option" readonly="not destination_delivery_address_id"
