<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--Actions-->
        <record id="logistics_storage_order_act_window" model="ir.actions.act_window">
            <field name="name">Storage Orders</field>
            <field name="res_model">logistics.storage.order</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{}</field>
            <field name="domain">[]</field>
        </record>


        <!--Views-->

        <record id="logistics_storage_order_list_view" model="ir.ui.view">
            <field name="name">logistics.storage.order.list.view</field>
            <field name="model">logistics.storage.order</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="logistics_storage_order_form_view" model="ir.ui.view">
            <field name="name">logistics.storage.order.form.view</field>
            <field name="model">logistics.storage.order</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


    </data>
</odoo>