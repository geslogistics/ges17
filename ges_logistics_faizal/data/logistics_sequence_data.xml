<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="logistics_customs_order_sequence" model="ir.sequence">
            <field name="name">Logistics Customs Order</field>
            <field name="code">logistics.customs.order</field>
            <field name="prefix">%(year)s/%(month)s/</field>
            <field name="padding">8</field>
            <field name="use_date_range" eval="False"/>
            <field name="implementation">standard</field>
        </record>

        <!--Sequence -->
        <record id="logistics_shipment_order_sequence" model="ir.sequence">
            <field name="name">Shipment Order</field>
            <field name="code">logistics.shipment.order</field>
            <field name="prefix">/%(year)s/%(month)s/</field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="logistics_transport_order_sequence" model="ir.sequence">
            <field name="name">Transport Order</field>
            <field name="code">logistics.transport.order</field>
            <field name="prefix">GES/TRO/%(year)s/%(month)s/</field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="logistics_storage_order_sequence" model="ir.sequence">
            <field name="name">Storage Order</field>
            <field name="code">logistics.storage.order</field>
            <field name="prefix">/%(year)s/%(month)s/</field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="logistics_customs_order_sequence" model="ir.sequence">
            <field name="name">Customs Order</field>
            <field name="code">logistics.customs.order</field>
            <field name="prefix">/%(year)s/%(month)s/</field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="logistics_service_order_sequence" model="ir.sequence">
            <field name="name">Service Order</field>
            <field name="code">logistics.service.order</field>
            <field name="prefix">/%(year)s/%(month)s/</field>
            <field name="padding">8</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--    Payment Request Sequence Number-->
        <record id="payment_request_number" model="ir.sequence">
            <field name="name">payment_request_number</field>
            <field name="code">payment.request.number</field>
            <field name="active">TRUE</field>
            <field name="prefix">GES/PR/%(range_year)s/%(month)s/</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>